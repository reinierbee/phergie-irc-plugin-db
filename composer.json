    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "dev-master",
        "doctrine/dbal": "2.3.5",
        "phpunit/phpunit": "4.3.1",
        "phake/phake": "v2.0.0-beta2"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\Db\\": "src"
