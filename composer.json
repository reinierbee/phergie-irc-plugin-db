{
    "name": "phergie/phergie-irc-plugin-react-db",
    "description": "Phergie plugin for adding phergie events to database",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "reinier@reinierboon.com",
        "irc": "irc://irc.freenode.net/phergie",
